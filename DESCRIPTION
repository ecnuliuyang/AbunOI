Package: AbunOI
Type: Package
Title: Semiparametric empirical likelihood inference for abundance from one-inflated capture-recapture data
Version: 0.1-1
Author: c(person("Liu", "Yang",
            email = "liuyangecnu@163.com", 
            role=c("aut","cre")),
            person("Liu", "Yukun",
            email = "ykliu@sfs.ecnu.edu.cn", 
            role=c("aut")),
            person("Li", "Pengfei",
            email = "pengfei.li@uwaterloo.ca", 
